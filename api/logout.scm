(module api racket/base
	(require
		racket/contract
		"../library/network.scm"
		"../packet/game/client/logout.scm"
	)
	(provide logout)
	
	(define (logout connection) ; Ќе может быть синхронным, т.к. можно пропустить некоторые пакеты и получить отказ
		(send connection (game-client-packet/logout))
	)
)